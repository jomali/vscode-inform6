{
    "name": "inform-6",
    "displayName": "Inform 6",
    "description": "Inform 6 extension for VS Code",
    "version": "0.1.1",
    "homepage": "https://bitbucket.org/Natrium729/vscode-inform6/",
    "bugs": "https://bitbucket.org/Natrium729/vscode-inform6/issues?status=new&status=open",
    "license": "MIT",
    "author": "NathanaÃ«l Marion <natrium729@gmail.com> (http://ulukos.com/)",
    "publisher": "natrium729",
    "repository": "https://gitlab.com/Natrium729/vscode-inform6.git",
    "engines": {
        "vscode": "^1.36.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "inform6",
                "aliases": ["Inform 6", "inform6"],
                "extensions": [".inf",".h"],
                "configuration": "./language-configuration.json"
            },
            {
                "id": "inform6template",
                "aliases": ["Inform 6 Template", "inform6template"],
                "extensions": [".i6t"],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "inform6",
                "scopeName": "source.inform6",
                "path": "./syntaxes/inform6.tmLanguage.json",
                "embeddedLanguages": {
                    "meta.embedded.js": "javascript"
                }
            },
            {
                "language": "inform6template",
                "scopeName": "source.inform6.template",
                "path": "./syntaxes/inform6template.tmLanguage.json"
            },
            {
                "scopeName": "inform6template.injection",
                "path": "./syntaxes/inform6templateinjection.json",
                "injectTo": ["source.inform6.template"]
            }
        ]
    }
}
